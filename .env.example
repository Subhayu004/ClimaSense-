# Environment Variables - DO NOT COMMIT THIS FILE!
# Copy this to .env and add your actual API keys

# Google Maps API (required for heatmap visualization)
# Enable the "Maps JavaScript API" and "Visualization Library" in Google Cloud Console
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Gemini API
GEMINI_API_KEY=

# Groq API (for climate risk analysis)
GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE

# AWS API Gateway (if using)
AWS_API_ENDPOINT=YOUR_AWS_ENDPOINT_HERE

# OpenWeatherMap API (if using)
OPENWEATHER_API_KEY=YOUR_OPENWEATHER_KEY_HERE

# Server Configuration
PORT=3000
NODE_ENV=production
